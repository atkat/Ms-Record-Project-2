<a href="/logout">Log Out</a>

<h1>Hey {{user.username}}</h1>
<div class="aboutme">
  <h3>About me:</h3>
  <p>{{user.aboutMe}}</p>
</div>

<a href="/wishlist">Wishlist ({{user.wishList.length}})</a>

<div class="collection-list">
  {{#each records}}
  <div class="album-card">
    <img src="{{thumb}}" alt="">
    <h2>{{artists_sort}}</h2>
    <h3>{{title}}</h3>
    <button action="/artist/:id/delete" method="POST">Remove from collection</button>
  </div>
  {{/each}}
</div>